{
  "presets": [
    ["env",
      {
        "targets": {
          "ie": 9,
          "uglify": true
        },
        "useBuiltIns": false,
        "modules": false
      }
    ]
  ],
  "plugins": [
    ["transform-object-rest-spread", {
      "useBuiltIns": true
    }],
    [
      "transform-runtime",
      {
        "polyfill": false
      }
    ]
  ]
}