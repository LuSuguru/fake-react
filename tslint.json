{
  "extends": "tslint:recommended",
  "rules": {
    "max-line-length": {
      "options": [
        200
      ]
    },
    "object-literal-sort-keys": false,
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": false,
    "no-conditional-assignment": false,
    "no-consecutive-blank-lines": false,
    "no-console": {
      "severity": "warning",
      "options": [
        "debug",
        "info",
        "log",
        "time",
        "timeEnd",
        "trace"
      ]
    },
    "semicolon": [
      true,
      "never"
    ],
    "quotemark": [
      true,
      "single"
    ],
    "ban-types": false,
    "member-access": false,
    "interface-name": false
  },
  "jsRules": {
    "max-line-length": {
      "options": [
        120
      ]
    }
  }
}